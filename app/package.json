{
  "name": "app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "production": "node ./build/index.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint --fix --ext .ts .",
    "watch": "nodemon --exec \"npm run lint && npm run dev\"",
    "knex:migrate": "npx knex --knexfile ./src/server/database/knex/enviroment.ts migrate:latest",
    "knex:rollback": "npx knex --knexfile ./src/server/database/knex/enviroment.ts migrate:rollback",
    "knex:rollback-all": "npx knex --knexfile ./src/server/database/knex/enviroment.ts migrate:rollback --all",
    "knex:seed": "npx knex --knexfile ./src/server/database/knex/enviroment.ts seed:run"
  },
  "keywords": [],
  "author": "Gustavo Teixeira Pardini",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "knex": "^3.1.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
